{
    "name": "grechanyuk/yandexweather",
    "description": ":description",
    "license": "MIT",
    "authors": [
        {
            "name": "Egor G.",
            "email": "xorolllo@hotmail.com"
        }
    ],
    "homepage": "https://github.com/grechanyuk/yandexweather",
    "keywords": ["Laravel", "YandexWeather"],
    "require": {
        "illuminate/support": "~5",
        "guzzlehttp/guzzle": "^6.3",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Grechanyuk\\YandexWeather\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Grechanyuk\\YandexWeather\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Grechanyuk\\YandexWeather\\YandexWeatherServiceProvider"
            ],
            "aliases": {
                "YandexWeather": "Grechanyuk\\YandexWeather\\Facades\\YandexWeather"
            }
        }
    }
}
